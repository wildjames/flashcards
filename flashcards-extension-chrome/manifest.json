{
    "name": "Jim's Flashcards",
    "description": "Sometimes opens a flashcard in a new tab",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}